{
    "name": "snchz_scripts/banking",
    "description": "Snchz Packages",
    "type": "library",
    "autoload": {
        "files": [
            "src/Multipay/helpers.php",
            "src/Exchanger/helpers.php"
        ],
        "psr-4": {
            "SnchzScripts\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rick Snchz",
            "email": "es@snchz.me"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/support" : ">=8.0",
        "illuminate/console" : ">=8.0",
        "illuminate/cache" : ">=8.0",
        "illuminate/database" : ">=8.0",
        "illuminate/http" : ">=8.0",
        "illuminate/routing" : ">=8.0",
        "akaunting/laravel-money": "2.*",
        "paypal/paypal-checkout-sdk": "^1.0",
        "stripe/stripe-php": "^7.94",
        "openpayu/openpayu" : "2.3.*",
        "mollie/mollie-api-php": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akaunting/laravel-money"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "SnchzScripts\\Multipay\\MultipayServiceProvider",
                "SnchzScripts\\Landing\\LandingServiceProvider",
                "SnchzScripts\\Exchanger\\ExchangerServiceProvider"
            ]
        }
    }
}
